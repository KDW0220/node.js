<table id="tbl" style="height: fit-content;"></table>

<script id="temp" type="text/x-handlebars-template">
    <tr>
        <td colspan="5">
          <button onclick="location.href='/camp/insert'">캠핑장등록</button>
        </td>
      </tr>
  <tr class="top">
    <td width=100>캠핑장번호</td>
    <td width=100>캠핑장이름</td>
    <td width=200>전화번호</td>
    <td width=150>주소</td>
    <td width=100>삭제</td>
  </tr>
  {{#each.}}
  <tr class="row" onclick="location.href='/camp/read?cid={{cid}}'">
    <td>{{cid}}</td>
    <td>{{name}}</td>
    <td>{{tel}}</td>
    <td>{{address}}</td>
    <td><button class = "delete">삭제</button></td>
  </tr>
  {{/each}}     
</script>
<script>
    getCamp();
    function getCamp(){
      $.ajax({
        type : "get",
        dataType : "json",
        url : "/camp/list",
        success:function(data){
          var template = Handlebars.compile($("#temp").html());
          $("#tbl").html(template(data));
        }
      });
    }
  </script>